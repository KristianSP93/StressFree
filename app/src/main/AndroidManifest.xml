<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kristian.stressfree">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Views.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Views.CreateUserActivity" />
        <activity android:name=".Views.MainActivity" />
        <activity android:name=".Views.SettingsActivity" />
        <activity android:name=".Views.SoundActivity" />
        <activity android:name=".Views.PictureActivity" />
        <activity android:name=".Views.MindfulnessActivity" />
        <activity android:name=".Views.VideoActivity" />
        <activity android:name=".Views.MovementActivity" />
        <activity android:name=".Views.MyStressFreeActivity" />
        <activity
            android:name=".Utilities.FullscreenPicActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen_pic"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>